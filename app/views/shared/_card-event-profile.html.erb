<%= link_to event_path(event), class: 'card-a' do %>
  <div class="card-event my-3 shadow">
    <%= cl_image_tag event.photo.key, alt: "#", width: 200 %>
    <div class="card-details">
    <div class="user-card-info">
      <h1> <%= event.name %> </h1>
      <p><%= event.description %></p>
    </div>
    <div class="category">
      <%= event.category %>
    </div>
    </div>
      <div class="date">
        <strong><%= event.start_time.strftime("%m/%d/%Y")  %></strong>
      </div>
  </div>
<% end %>
